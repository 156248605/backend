<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.elex.oa.dao.hr.IHRsetGjjgscdDao">

	<resultMap id="GjjgscdMapper" type="HRsetGjjgscd">
		<id column="id" property="id" />
		<result column="gjjgscd" property="gjjgscd" />
	</resultMap>

	<insert id="insertOne" parameterType="HRsetGjjgscd" useGeneratedKeys="true" keyProperty="id">
		insert into tb_hr_set_gjjgscd (
			id,
			gjjgscd)
		values (
			NULL,
			#{gjjgscd})
	</insert>

	<select id="selectAll" resultMap="GjjgscdMapper">
		SELECT * FROM tb_hr_set_gjjgscd
		ORDER BY id DESC
	</select>

	<select id="selectByConditions" parameterType="HRsetGjjgscd" resultMap="GjjgscdMapper">
		SELECT * FROM tb_hr_set_gjjgscd
		<where>
			<if test="id!=null and id!=''">AND id=#{id}</if>
			<if test="gjjgscd!=null and gjjgscd!=''">AND gjjgscd=#{gjjgscd}</if>
		</where>
	</select>

	<delete id="deleteOne" parameterType="java.lang.Integer">
		DELETE FROM tb_hr_set_gjjgscd WHERE id=#{id}
	</delete>

	<update id="updateOne" parameterType="HRsetFunctionalType">
		UPDATE tb_hr_set_gjjgscd
		SET gjjgscd=#{gjjgscd}
		WHERE id=#{id}
	</update>
</mapper>
