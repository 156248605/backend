<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.elex.oa.dao.hr.IHRsetSsbgscdDao">

	<resultMap id="SsbgscdMapper" type="HRsetSsbgscd">
		<id column="id" property="id" />
		<result column="ssbgscd" property="ssbgscd" />
	</resultMap>

	<insert id="insertOne" parameterType="HRsetSsbgscd" useGeneratedKeys="true" keyProperty="id">
		insert into tb_hr_set_ssbgscd (
			id,
			ssbgscd)
		values (
			NULL,
			#{ssbgscd})
	</insert>

	<select id="selectAll" resultMap="SsbgscdMapper">
		SELECT * FROM tb_hr_set_ssbgscd
		ORDER BY id DESC
	</select>

	<select id="selectByConditions" parameterType="HRsetSsbgscd" resultMap="SsbgscdMapper">
		SELECT * FROM tb_hr_set_ssbgscd
		<where>
			<if test="id!=null and id!=''">AND id=#{id}</if>
			<if test="ssbgscd!=null and ssbgscd!=''">AND ssbgscd=#{ssbgscd}</if>
		</where>
	</select>

	<delete id="deleteOne" parameterType="java.lang.Integer">
		DELETE FROM tb_hr_set_ssbgscd WHERE id=#{id}
	</delete>

	<update id="updateOne" parameterType="HRsetFunctionalType">
		UPDATE tb_hr_set_ssbgscd
		SET ssbgscd=#{ssbgscd}
		WHERE id=#{id}
	</update>
</mapper>
