<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.elex.oa.dao.hr_shiyun.IHRsetZzmmDao">

	<resultMap id="ZzmmMapper" type="HRsetZzmm">
		<id column="id" property="id" />
		<result column="zzmm" property="zzmm" />
	</resultMap>

	<insert id="insertOne" parameterType="HRsetZzmm" useGeneratedKeys="true" keyProperty="id">
		insert into tb_hr_set_zzmm (
			id,
			zzmm)
		values (
			NULL,
			#{zzmm})
	</insert>

	<select id="selectAll" resultMap="ZzmmMapper">
		SELECT * FROM tb_hr_set_zzmm
		ORDER BY id DESC
	</select>

	<select id="selectByConditions" parameterType="HRsetZzmm" resultMap="ZzmmMapper">
		SELECT * FROM tb_hr_set_zzmm
		<where>
			<if test="id!=null and id!=''">AND id=#{id}</if>
			<if test="zzmm!=null and zzmm!=''">AND zzmm=#{zzmm}</if>
		</where>
	</select>

	<delete id="deleteOne" parameterType="java.lang.Integer">
		DELETE FROM tb_hr_set_zzmm WHERE id=#{id}
	</delete>

	<update id="updateOne" parameterType="HRsetFunctionalType">
		UPDATE tb_hr_set_zzmm
		SET zzmm=#{zzmm}
		WHERE id=#{id}
	</update>
</mapper>
