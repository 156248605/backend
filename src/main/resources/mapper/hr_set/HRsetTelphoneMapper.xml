<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.elex.oa.dao.hr.IHRsetTelphoneDao">

	<resultMap id="TelphoneMapper" type="HRsetTelphone">
		<id column="id" property="id" />
		<result column="telphone" property="telphone" />
	</resultMap>

	<insert id="insertOne" parameterType="HRsetTelphone" useGeneratedKeys="true" keyProperty="id">
		insert into tb_hr_set_telphone (
			id,
			telphone)
		values (
			NULL,
			#{telphone})
	</insert>

	<select id="selectAll" resultMap="TelphoneMapper">
		SELECT * FROM tb_hr_set_telphone
		ORDER BY id DESC
	</select>

	<select id="selectByConditions" parameterType="HRsetTelphone" resultMap="TelphoneMapper">
		SELECT * FROM tb_hr_set_telphone
		<where>
			<if test="id!=null and id!=''">AND id=#{id}</if>
			<if test="telphone!=null and telphone!=''">AND telphone=#{telphone}</if>
		</where>
	</select>

	<delete id="deleteOne" parameterType="java.lang.Integer">
		DELETE FROM tb_hr_set_telphone WHERE id=#{id}
	</delete>

	<update id="updateOne" parameterType="HRsetFunctionalType">
		UPDATE tb_hr_set_telphone
		SET telphone=#{telphone}
		WHERE id=#{id}
	</update>
</mapper>
