<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.elex.oa.dao.hr.IHRsetZgxlDao">

	<resultMap id="ZgxlMapper" type="HRsetZgxl">
		<id column="id" property="id" />
		<result column="zgxl" property="zgxl" />
	</resultMap>

	<insert id="insertOne" parameterType="HRsetZgxl" useGeneratedKeys="true" keyProperty="id">
		insert into tb_hr_set_zgxl (
			id,
			zgxl)
		values (
			NULL,
			#{zgxl})
	</insert>

	<select id="selectAll" resultMap="ZgxlMapper">
		SELECT * FROM tb_hr_set_zgxl
		ORDER BY id DESC
	</select>

	<select id="selectByConditions" parameterType="HRsetZgxl" resultMap="ZgxlMapper">
		SELECT * FROM tb_hr_set_zgxl
		<where>
			<if test="id!=null and id!=''">AND id=#{id}</if>
			<if test="zgxl!=null and zgxl!=''">AND zgxl=#{zgxl}</if>
		</where>
	</select>

	<delete id="deleteOne" parameterType="java.lang.Integer">
		DELETE FROM tb_hr_set_zgxl WHERE id=#{id}
	</delete>

	<update id="updateOne" parameterType="HRsetFunctionalType">
		UPDATE tb_hr_set_zgxl
		SET zgxl=#{zgxl}
		WHERE id=#{id}
	</update>
</mapper>
