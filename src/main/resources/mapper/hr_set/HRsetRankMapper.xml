<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.elex.oa.dao.hr_shiyun.IHRsetRankDao">

	<resultMap id="RankMapper" type="HRsetRank">
		<id column="id" property="id" />
		<result column="rank" property="rank" />
	</resultMap>

	<insert id="insertOne" parameterType="HRsetRank" useGeneratedKeys="true" keyProperty="id">
		insert into tb_hr_set_rank (
			id,
			rank)
		values (
			NULL,
			#{rank})
	</insert>

	<select id="selectAll" resultMap="RankMapper">
		SELECT * FROM tb_hr_set_rank
		ORDER BY id DESC
	</select>

	<select id="selectByConditions" parameterType="HRsetRank" resultMap="RankMapper">
		SELECT * FROM tb_hr_set_rank
		<where>
			<if test="id!=null and id!=''">AND id=#{id}</if>
			<if test="rank!=null and rank!=''">AND rank=#{rank}</if>
		</where>
	</select>

	<delete id="deleteOne" parameterType="java.lang.Integer">
		DELETE FROM tb_hr_set_rank WHERE id=#{id}
	</delete>

	<update id="updateOne" parameterType="HRsetFunctionalType">
		UPDATE tb_hr_set_rank
		SET rank=#{rank}
		WHERE id=#{id}
	</update>
</mapper>
