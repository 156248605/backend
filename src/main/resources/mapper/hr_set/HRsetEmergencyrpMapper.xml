<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.elex.oa.dao.hr.IHRsetEmergencyrpDao">

	<resultMap id="EmergencyrpMapper" type="HRsetEmergencyrp">
		<id column="id" property="id" />
		<result column="emergencyrp" property="emergencyrp" />
	</resultMap>

	<insert id="insertOne" parameterType="HRsetEmergencyrp" useGeneratedKeys="true" keyProperty="id">
		insert into tb_hr_set_emergencyrp (
			id,
			emergencyrp)
		values (
			NULL,
			#{emergencyrp})
	</insert>

	<select id="selectAll" resultMap="EmergencyrpMapper">
		SELECT * FROM tb_hr_set_emergencyrp
		ORDER BY id DESC
	</select>

	<select id="selectByConditions" parameterType="HRsetEmergencyrp" resultMap="EmergencyrpMapper">
		SELECT * FROM tb_hr_set_emergencyrp
		<where>
			<if test="id!=null and id!=''">AND id=#{id}</if>
			<if test="emergencyrp!=null and emergencyrp!=''">AND emergencyrp=#{emergencyrp}</if>
		</where>
	</select>

	<delete id="deleteOne" parameterType="java.lang.Integer">
		DELETE FROM tb_hr_set_emergencyrp WHERE id=#{id}
	</delete>

	<update id="updateOne" parameterType="HRsetFunctionalType">
		UPDATE tb_hr_set_emergencyrp
		SET emergencyrp=#{emergencyrp}
		WHERE id=#{id}
	</update>
</mapper>
