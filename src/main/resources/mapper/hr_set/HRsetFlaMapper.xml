<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.elex.oa.dao.hr_shiyun.IHRsetFlaDao">

	<resultMap id="FlaMapper" type="HRsetFla">
		<id column="id" property="id" />
		<result column="fla" property="fla" />
	</resultMap>

	<insert id="insertOne" parameterType="HRsetFla" useGeneratedKeys="true" keyProperty="id">
		insert into tb_hr_set_fla (
			id,
			fla)
		values (
			NULL,
			#{fla})
	</insert>

	<select id="selectAll" resultMap="FlaMapper">
		SELECT * FROM tb_hr_set_fla
		ORDER BY id DESC
	</select>

	<select id="selectByConditions" parameterType="HRsetFla" resultMap="FlaMapper">
		SELECT * FROM tb_hr_set_fla
		<where>
			<if test="id!=null and id!=''">AND id=#{id}</if>
			<if test="fla!=null and fla!=''">AND fla=#{fla}</if>
		</where>
	</select>

	<delete id="deleteOne" parameterType="java.lang.Integer">
		DELETE FROM tb_hr_set_fla WHERE id=#{id}
	</delete>

	<update id="updateOne" parameterType="HRsetFunctionalType">
		UPDATE tb_hr_set_fla
		SET fla=#{fla}
		WHERE id=#{id}
	</update>
</mapper>
