<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.elex.oa.dao.hr.IHRsetResourcetypeDao">

	<resultMap id="ResourcetypeMapper" type="HRsetResourcetype">
		<id column="id" property="id" />
		<result column="resourcetype" property="resourcetype" />
	</resultMap>

	<insert id="insertOne" parameterType="HRsetResourcetype" useGeneratedKeys="true" keyProperty="id">
		insert into tb_hr_set_resourcetype (
			id,
			resourcetype)
		values (
			NULL,
			#{resourcetype})
	</insert>

	<select id="selectAll" resultMap="ResourcetypeMapper">
		SELECT * FROM tb_hr_set_resourcetype
		ORDER BY id DESC
	</select>

	<select id="selectByConditions" parameterType="HRsetResourcetype" resultMap="ResourcetypeMapper">
		SELECT * FROM tb_hr_set_resourcetype
		<where>
			<if test="id!=null and id!=''">AND id=#{id}</if>
			<if test="resourcetype!=null and resourcetype!=''">AND resourcetype=#{resourcetype}</if>
		</where>
	</select>

	<delete id="deleteOne" parameterType="java.lang.Integer">
		DELETE FROM tb_hr_set_resourcetype WHERE id=#{id}
	</delete>

	<update id="updateOne" parameterType="HRsetResourcetype">
		UPDATE tb_hr_set_resourcetype
		SET resourcetype=#{resourcetype}
		WHERE id=#{id}
	</update>

</mapper>
