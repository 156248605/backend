<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.elex.oa.dao.hr_shiyun.IHRsetContracttypeDao">

	<resultMap id="ContracttypeMapper" type="HRsetContracttype">
		<id column="id" property="id" />
		<result column="contracttype" property="contracttype" />
	</resultMap>

	<insert id="insertOne" parameterType="HRsetContracttype" useGeneratedKeys="true" keyProperty="id">
		insert into tb_hr_set_contracttype (
			id,
			contracttype)
		values (
			NULL,
			#{contracttype})
	</insert>

	<select id="selectAll" resultMap="ContracttypeMapper">
		SELECT * FROM tb_hr_set_contracttype
		ORDER BY id DESC
	</select>

	<select id="selectByConditions" parameterType="HRsetContracttype" resultMap="ContracttypeMapper">
		SELECT * FROM tb_hr_set_contracttype
		<where>
			<if test="id!=null and id!=''">AND id=#{id}</if>
			<if test="contracttype!=null and contracttype!=''">AND contracttype=#{contracttype}</if>
		</where>
	</select>

	<select id="selectById" parameterType="java.lang.Integer" resultMap="ContracttypeMapper">
		SELECT * FROM tb_hr_set_contracttype
		WHERE id=#{id}
	</select>

	<delete id="deleteOne" parameterType="java.lang.Integer">
		DELETE FROM tb_hr_set_contracttype WHERE id=#{id}
	</delete>

	<update id="updateOne" parameterType="HRsetFunctionalType">
		UPDATE tb_hr_set_contracttype
		SET contracttype=#{contracttype}
		WHERE id=#{id}
	</update>
</mapper>
