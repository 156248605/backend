<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.elex.oa.dao.dao_shiyun.IHRsetChildrenDao">

	<resultMap id="ChildrenMapper" type="HRsetChildren">
		<id column="id" property="id" />
		<result column="children" property="children" />
	</resultMap>

	<insert id="insertOne" parameterType="HRsetChildren" useGeneratedKeys="true" keyProperty="id">
		insert into tb_hr_set_children (
			id,
			children)
		values (
			NULL,
			#{children})
	</insert>

	<select id="selectAll" resultMap="ChildrenMapper">
		SELECT * FROM tb_hr_set_children
		ORDER BY id DESC
	</select>

	<select id="selectByConditions" parameterType="HRsetChildren" resultMap="ChildrenMapper">
		SELECT * FROM tb_hr_set_children
		<where>
			<if test="id!=null and id!=''">AND id=#{id}</if>
			<if test="children!=null and children!=''">AND children=#{children}</if>
		</where>
	</select>

	<delete id="deleteOne" parameterType="java.lang.Integer">
		DELETE FROM tb_hr_set_children WHERE id=#{id}
	</delete>

	<update id="updateOne" parameterType="HRsetFunctionalType">
		UPDATE tb_hr_set_children
		SET children=#{children}
		WHERE id=#{id}
	</update>
</mapper>
