<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.elex.oa.dao.dao_shiyun.IHRsetZyzsnameDao">

	<resultMap id="ZyzsnameMapper" type="HRsetZyzsname">
		<id column="id" property="id" />
		<result column="zyzsname" property="zyzsname" />
	</resultMap>

	<insert id="insertOne" parameterType="HRsetZyzsname" useGeneratedKeys="true" keyProperty="id">
		insert into tb_hr_set_zyzsname (
			id,
			zyzsname)
		values (
			NULL,
			#{zyzsname})
	</insert>

	<select id="selectAll" resultMap="ZyzsnameMapper">
		SELECT * FROM tb_hr_set_zyzsname
		ORDER BY id DESC
	</select>

	<select id="selectByConditions" parameterType="HRsetZyzsname" resultMap="ZyzsnameMapper">
		SELECT * FROM tb_hr_set_zyzsname
		<where>
			<if test="id!=null and id!=''">AND id=#{id}</if>
			<if test="zyzsname!=null and zyzsname!=''">AND zyzsname=#{zyzsname}</if>
		</where>
	</select>

	<delete id="deleteOne" parameterType="java.lang.Integer">
		DELETE FROM tb_hr_set_zyzsname WHERE id=#{id}
	</delete>

	<update id="updateOne" parameterType="HRsetFunctionalType">
		UPDATE tb_hr_set_zyzsname
		SET zyzsname=#{zyzsname}
		WHERE id=#{id}
	</update>
</mapper>
