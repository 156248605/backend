<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.elex.oa.dao.dao_shiyun.IHRsetSxzyDao">

	<resultMap id="SxzyMapper" type="HRsetSxzy">
		<id column="id" property="id" />
		<result column="sxzy" property="sxzy" />
	</resultMap>

	<insert id="insertOne" parameterType="HRsetSxzy" useGeneratedKeys="true" keyProperty="id">
		insert into tb_hr_set_sxzy(
			id,
			sxzy)
		values (
			NULL,
			#{sxzy})
	</insert>

	<select id="selectAll" resultMap="SxzyMapper">
		SELECT * FROM tb_hr_set_sxzy
		ORDER BY id DESC
	</select>

	<select id="selectByConditions" parameterType="HRsetSxzy" resultMap="SxzyMapper">
		SELECT * FROM tb_hr_set_sxzy
		<where>
			<if test="id!=null and id!=''">AND id=#{id}</if>
			<if test="sxzy!=null and sxzy!=''">AND sxzy=#{sxzy}</if>
		</where>
	</select>

	<delete id="deleteOne" parameterType="java.lang.Integer">
		DELETE FROM tb_hr_set_sxzy WHERE id=#{id}
	</delete>

	<update id="updateOne" parameterType="HRsetFunctionalType">
		UPDATE tb_hr_set_sxzy
		SET sxzy=#{sxzy}
		WHERE id=#{id}
	</update>
</mapper>
