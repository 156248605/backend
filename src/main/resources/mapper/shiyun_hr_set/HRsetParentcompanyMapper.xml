<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.elex.oa.dao.dao_shiyun.IHRsetParentcompanyDao">

	<resultMap id="ParentcompanyMapper" type="HRsetParentcompany">
		<id column="id" property="id" />
		<result column="parentcompanyname" property="parentcompanyname" />
	</resultMap>

	<insert id="insertOne" parameterType="HRsetParentcompany" useGeneratedKeys="true" keyProperty="id">
		insert into tb_hr_set_parentcompanyname (
			id,
			parentcompanyname)
		values (
			NULL,
			#{parentcompanyname})
	</insert>

	<select id="selectAll" resultMap="ParentcompanyMapper">
		SELECT * FROM tb_hr_set_parentcompanyname
		ORDER BY id DESC
	</select>

	<select id="selectByConditions" parameterType="HRsetParentcompany" resultMap="ParentcompanyMapper">
		SELECT * FROM tb_hr_set_parentcompanyname
		<where>
			<if test="id!=null and id!=''">AND id=#{id}</if>
			<if test="parentcompanyname!=null and parentcompanyname!=''">AND parentcompanyname=#{parentcompanyname}</if>
		</where>
	</select>

	<delete id="deleteOne" parameterType="java.lang.Integer">
		DELETE FROM tb_hr_set_parentcompanyname WHERE id=#{id}
	</delete>

	<update id="updateOne" parameterType="HRsetFunctionalType">
		UPDATE tb_hr_set_parentcompanyname
		SET parentcompanyname=#{parentcompanyname}
		WHERE id=#{id}
	</update>
</mapper>
