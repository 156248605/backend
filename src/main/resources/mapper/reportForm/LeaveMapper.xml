<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.elex.oa.dao.reportForm.LeaveDao">

    <select id="getLeaveForm" resultType="HashMap">
        SELECT * FROM w_qjsqd1 WHERE
        <if test="arg0 != null and arg0 != ''">
            F_SQR1 = #{arg0}
        </if>
        <if test="arg1 != null and arg1 != ''">
            AND F_KSSJ >= #{arg1}
        </if>
        <if test="arg2 != null and arg2 != ''">
            AND F_JSSJ &lt;= #{arg2}
        </if>
        <if test="arg3 != null and arg3 != ''">
            AND F_XMBH = #{arg3}
        </if>
        <if test="arg4 != null and arg4 != ''">
            AND F_QJXL = #{arg4}
        </if>
        <if test="arg5 != null and arg5 != ''">
            AND F_SZBM = #{arg5}
        </if>
    </select>

</mapper>